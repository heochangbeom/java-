package day3.remotecontroll;

public class MyClass {

	RemoteControl rc;

	public MyClass(RemoteControl rc) {
		super();
		this.rc = rc;
	}
	
	void methodA() {
		RemoteControl rc = new Audio();
		rc.turnOn();
		rc.setVolume(5);
	}
	
	void methodB(RemoteControl rc) {
		rc.turnOn();
		rc.setVolume(3);
	}
}
