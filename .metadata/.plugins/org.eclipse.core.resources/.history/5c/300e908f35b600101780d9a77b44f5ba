package day3.emp;

import java.util.stream.Stream;

public class 스트림 {
	public static void main(String[] args) {
		String[] strArr = {"키위", "포도", "바나나", "사과", "감", "사과"};
		
		Stream.of(strArr)
			  .sorted()
			  .limit(3)
			  .forEach(System.out::println);
		
		//사과가 몇개 있나? filter   count
		Stream.of(strArr)
		  .sorted()
		  .filter( str1 -> str1.equals("사과"))
		  .count;
		
		//List<String>
	}
}
