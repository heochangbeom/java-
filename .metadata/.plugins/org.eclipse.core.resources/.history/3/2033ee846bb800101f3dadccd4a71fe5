package day4;

import java.util.InputMismatchException;

public class Job1  extends Thread{

	@Override
	public void run() {
		for(int i=1; i<100; i++) {
			System.out.println(i);
				try {
					Thread.sleep(400);
				} catch (InterruptedException e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				}

		}
		super.run();
	}
	
	

}
