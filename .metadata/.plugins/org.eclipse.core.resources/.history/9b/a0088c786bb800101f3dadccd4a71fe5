package day4;

import java.util.InputMismatchException;

public class Job1  extends Thread{

	@Override
	public void run() {
		for(int i=1; i<10; i++) {
			System.out.println(i);
			try {
				Thread.sleep(400);
			} catch (InputMismatchException e) {
				e.printStackTrace();
			}
		}
		super.run();
	}
	
	

}
