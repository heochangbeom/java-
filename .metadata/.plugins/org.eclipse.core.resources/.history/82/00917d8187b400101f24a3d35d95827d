package day1;

public class 데이터타입 {
	public static void main(String[] args) {
//	1. 반려견의 이름을 name 변수에 저장하세요
	String name = "뭉뭉";
		
//	2. 반려견의 나이를 age 변수에 저장하세요
	long age = 4;
		
//	3. 반려견의 몸무게를 weight 변수에 저장하세요
	double weight = 12.4;
		
	System.out.println("나의 반려견 이름은 :" + name);
	System.out.println("나의 반려견 나이는 :" + age);
	System.out.println("나의 반려견 이름은 :" + weight); 

	System.out.println(13%2); 
	System.out.println(1%3); 
	System.out.println(2%3); 
	System.out.println(2%5); 
	System.out.println("가위:" + 4%3); 
	System.out.println("바위:" + 5%3); 
	System.out.println("보:" + 6%3); 
	
	
	
}

}
