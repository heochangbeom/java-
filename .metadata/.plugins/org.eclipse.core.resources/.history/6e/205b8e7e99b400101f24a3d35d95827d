package day1;

import java.util.ArrayList;

public class 로또번호 {
	public static void main(String[] args) {
		ArrayList<Integer> list = new ArrayList<Integer>();

		while (list.size() < 6) {
			int lotto = (int) (Math.random() * 3) + 1;

			list.add(lotto);

		}

		System.out.println(list);
	}// main
}// class
